modules = ["nodejs-18", "web", "nix"]
hidden = [".config", "package-lock.json"]
run = "npm run dev"

[nix]
channel = "stable-25_05"

[unitTest]
language = "nodejs"

[deployment]
run = ["sh", "-c", "npm run dev"]
deploymentTarget = "cloudrun"
ignorePorts = false

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 3001
